<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:background="@drawable/play_catch">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_alignParentStart="false">

        <ImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:id="@+id/home_catch"
            android:src="@drawable/symbol_home_play"
            android:layout_alignParentStart="true" />

        <ImageView
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:id="@+id/imageView11"
            android:layout_alignParentEnd="true"
            android:src="@drawable/symbol_settings_play" />

        <ImageView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:id="@+id/bonus_id"
            android:layout_toRightOf="@+id/home_catch"
            android:src="@drawable/play_catch_maze_stop"
            android:layout_marginLeft="5dp" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="560dp"
        android:layout_height="270dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/play_catch_homes"
        android:layout_centerHorizontal="true">

        <ImageButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/imageButton"
            android:adjustViewBounds="true"
            android:onClick="plus"
            android:background="@drawable/play_catch_mouse"
            android:layout_marginLeft="80dp"
            android:layout_marginTop="35dp" />

        <ImageButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/imageButton2"
            android:adjustViewBounds="true"
            android:onClick="plus"
            android:layout_marginLeft="175dp"
            android:layout_marginTop="35dp"
            android:background="@drawable/play_catch_mouse"
     />

        <ImageButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/imageButton3"
            android:adjustViewBounds="true"
            android:onClick="plus"
            android:background="@drawable/play_catch_mouse"
            android:layout_marginLeft="323dp"
            android:layout_marginTop="35dp"
     />

        <ImageButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/imageButton4"
            android:adjustViewBounds="true"
            android:onClick="plus"
            android:background="@drawable/play_catch_mouse"
            android:layout_marginLeft="418dp"
            android:layout_marginTop="35dp"
     />

        <ImageButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/imageButton5"
            android:adjustViewBounds="false"
            android:onClick="plus"
            android:background="@drawable/play_catch_mouse"
            android:layout_marginLeft="85dp"
            android:layout_marginTop="100dp"
             />

        <ImageButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/imageButton6"
            android:adjustViewBounds="true"
            android:onClick="plus"
            android:background="@drawable/play_catch_mouse"
            android:layout_marginLeft="170dp"
            android:layout_marginTop="100dp" />

        <ImageButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/imageButton7"
            android:adjustViewBounds="true"
            android:onClick="plus"
            android:background="@drawable/play_catch_mouse"
            android:layout_marginLeft="328dp"
            android:layout_marginTop="100dp"
            />

        <ImageButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/imageButton8"
            android:adjustViewBounds="true"
            android:onClick="plus"
            android:background="@drawable/play_catch_mouse"
            android:layout_marginLeft="415dp"
            android:layout_marginTop="100dp"
             />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:id="@+id/textView"
            android:layout_alignParentTop="false"
            android:layout_centerInParent="true"
            android:textColor="#164ce1" />
    </RelativeLayout>
</RelativeLayout>
